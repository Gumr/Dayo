.tryBtn1 .try2 {
  position:absolute;
  top:57rpx;
  left:24rpx;
  font-size:20rpx;
  width:2em;
}

.wxParse-inline {
    color: #0daf99;
}

.wxParse-p {
    font-size: 24rpx;
}

.closeL {
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    font-size: 50rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.focus {
    position: fixed;
    top: 44%;
    width: 284rpx;
    height: 130rpx;
}

.finger {
    position: fixed;
    top: 0%;
    left: 50%;
    transform: translate(-50%,-0%);
}

.footer {
  width: 100%;
  height: 50px;
  display: flex;
  align-items: center;
  border-top: 1rpx solid #B5B5B5;
}

.footer .fimage {
  width: 44rpx;
  height: 44rpx;
}

.footer .ftext {
  color: #8A8A8A;
  font-size: 20rpx;
  height: 44rpx;
  line-height: 44rpx;
}

.footer .ftext.active {
  color: #0DAF99;
}

.dialog {
  position: fixed;
  z-index: 1001;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}

.dialog .dialog-bg {
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0, .3);
}

.dialog .dialog-box {
  position: absolute;
  left: 0;
  bottom: 0;
  background: #fff;
  width: 100%;
}

.dialog .dialog-box .dialog-top {
  padding: 40rpx 28rpx;
  box-sizing: border-box;
} 
 
.dialog .dialog-box .dialog-bottom {
  display: flex;
  width: 100%;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid rgba(0, 0,0,.3);
  box-sizing: border-box;
  padding: 10rpx 28rpx;
  color: rgba(102, 102, 102, 1);
}

.dialog .dialog-box .dialog-bottom > view {
  font-size: 28rpx;
}

.cost-dialog .cost-title {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 10rpx;
}

.cost-dialog .cost-desc {
  font-size: 28rpx;
  color: rgba(102, 102, 102, 1);
}

.cost-dialog .cost-price {
  font-size: 36rpx;
  color: rgba(13, 175, 153, 1);
}

.cost-dialog .cost-button {
  width: 288rpx;
  height: 68rpx;
  line-height: 68rpx;
  background: rgba(13, 175, 153, 1);
  border-radius: 34rpx;
  font-size: 34rpx;
  color: #fff;
}
.wxParse-span{
  font-size:14px!important;
}
